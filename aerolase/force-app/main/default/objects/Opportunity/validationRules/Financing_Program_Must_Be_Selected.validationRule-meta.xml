<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Financing_Program_Must_Be_Selected</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(StageName, &apos;Contract Signed&apos;), ISBLANK(TEXT(  Financing_Program__c )))</errorConditionFormula>
    <errorMessage>Financing Type Needs to be selected from drop-down list</errorMessage>
</ValidationRule>
