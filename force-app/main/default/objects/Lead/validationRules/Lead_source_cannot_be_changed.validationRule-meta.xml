<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_source_cannot_be_changed</fullName>
    <active>false</active>
    <errorConditionFormula>ISCHANGED( LeadSource ) &amp;&amp; (PRIORVALUE(LeadSource) &lt;&gt; &quot;&quot;) &amp;&amp; !($User.Alias = &quot;sjohn&quot; )</errorConditionFormula>
    <errorDisplayField>LeadSource</errorDisplayField>
    <errorMessage>Lead source cannot be changed</errorMessage>
</ValidationRule>
